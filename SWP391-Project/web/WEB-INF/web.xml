<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserProfileServlet</servlet-name>
        <servlet-class>controller.UserProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edit_UserServlet</servlet-name>
        <servlet-class>controller.AdminServlet.EditUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>controller.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutServlet</servlet-name>
        <servlet-class>controller.logoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupServlet</servlet-name>
        <servlet-class>controller.signupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>roomDetailServlet</servlet-name>
        <servlet-class>controller.AdminServlet.roomDetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>roomAdminServlet</servlet-name>
        <servlet-class>controller.AdminServlet.roomAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userServlet</servlet-name>
        <servlet-class>controller.AdminServlet.userServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TryServlet</servlet-name>
        <servlet-class>controller.TryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>controller.SearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingListServlet</servlet-name>
        <servlet-class>controller.Booker.BookingListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlaceServlet</servlet-name>
        <servlet-class>controller.PlaceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeServlet</servlet-name>
        <servlet-class>controller.TypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserProfileServlet</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edit_UserServlet</servlet-name>
        <url-pattern>/edit_user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/homepage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signupServlet</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>roomDetailServlet</servlet-name>
        <url-pattern>/roomdetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>roomAdminServlet</servlet-name>
        <url-pattern>/roomadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TryServlet</servlet-name>
        <url-pattern>/try</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingListServlet</servlet-name>
        <url-pattern>/booking_list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlaceServlet</servlet-name>
        <url-pattern>/place</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeServlet</servlet-name>
        <url-pattern>/type</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
